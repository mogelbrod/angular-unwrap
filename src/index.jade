html(ng-app="unwrap")
  head
    script(src="//ajax.googleapis.com/ajax/libs/angularjs/1.3.0-beta.17/angular.js")
    script(src="index.js")
    script(src="unwrap.js")
  body(ng-controller="DemoCtrl")
    h1 unwrap() demo
    ul
      li(ng-repeat="(key, value) in demo")
        strong {{key}}:
        em  {{value.v || value.join(' ')}}
    h2 Event log
    ul.log
      li(ng-repeat="row in log")
        strong {{row.name}}:
        code  {{row.args | json}}
